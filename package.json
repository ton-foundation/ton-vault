{
  "name": "ton-vault",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/ton-foundation/ton-vault.git",
  "author": "Steve Korshakov <steve@korshakov.com>",
  "license": "MIT",
  "bin": "./build/index.js",
  "scripts": {
    "build": "rm -fr dist && rm -fr build && tsc && pkg .",
    "dev": "ts-node ./source/index.ts"
  },
  "dependencies": {
    "cli-table": "0.3.11",
    "date-fns": "2.28.0",
    "enquirer": "2.3.6",
    "fp-ts": "2.12.1",
    "io-ts": "2.2.16",
    "ora": "5.4.1",
    "qrcode-terminal": "0.12.0",
    "ton": "11.6.0",
    "ton-crypto": "3.2.0"
  },
  "devDependencies": {
    "@types/cli-table": "0.3.0",
    "@types/node": "17.0.33",
    "@types/qrcode-terminal": "0.12.0",
    "pkg": "5.6.0",
    "ts-node": "10.7.0",
    "typescript": "4.6.4"
  },
  "pkg": {
    "scripts": "source/**/*.js",
    "targets": [
      "node16-linux-arm64",
      "node16-linux-x64",
      "node16-macos-arm64",
      "node16-macos-x64",
      "node16-win-arm64",
      "node16-win-x64"
    ],
    "outputPath": "dist"
  }
}